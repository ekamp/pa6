#ifndef DATSTR_H
#define DATSTR_H

struct Order
{
	char * bookTitle;
	float cost;
	float currentCredits;
}


struct Node
{
	void * data;
	Node * next;
}

struct User{
	char * username;
	int uid;
	double remainingCedits;
	pthread_mutex_t userMutex;
	struct Node * success;
	struct Node * fail;
};

#endif
